<!DOCTYPE html>
<html lang="en">

<head>
  <title><%= pageTitle %> | Error <%= code %> </title>
  <link rel="stylesheet" href="/css/styles.css">
</head>

<body class="error">

  <%- include("./partials/headerBackToIndex.ejs"); _%>

  <main>
    <div class="main-container animate-children fade-in-and-drop">
      <h2 class="text-gradient"><%= code %></h2>
      <%_ if (locals.message ) { _%><p><%= locals.message %></p><%_ } _%>
      <%_ if (locals.partial ) { _%><%- include(process.cwd() + locals.partial) %><%_ } _%>
    </div>
  </main>

  <%- include("./partials/creditFooter.ejs"); _%>
  <%- include("./partials/flashDialog.ejs"); _%>
</body>

</html>